<form
  {{#if numerology.index}}
  hx-post="/settings/numerology/{{ numerology.index }}"
  {{else}}
  hx-post="/settings/numerology/add"
  {{/if}}
  hx-encoding="multipart/form-data"
  hx-target="#numerology"
  hx-swap="innerHTML"
  hx-on::send-error="alert('Unable to contact Helipad')"
  hx-on::response-error="alert(`${event.detail.error}\n${event.detail.xhr.responseText}`)"
  hx-on::after-request="(event.detail.xhr.status == 200) && $(this).closest('.modal').modal('hide')"
>
<div class="modal-dialog">
  <div class="modal-content">
    <div class="modal-header">
      {{#if numerology.index}}
        <h5 class="modal-title">Edit Numerology</h5>
      {{else}}
        <h5 class="modal-title">Add Numerology</h5>
      {{/if}}
      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
    <div class="modal-body">
      <div class="form-group">
        <label for="num-amount">Amount</label>
        <div class="form-inline">
          <select name="equality" class="form-control w-auto">
            <option value="="{{#if equality.eq}} selected{{/if}}>Equal to</option>
            <option value="=~"{{#if equality.in}} selected{{/if}}>Contains</option>
            <option value="<"{{#if equality.lt}} selected{{/if}}>Less than</option>
            <option value=">="{{#if equality.gte}} selected{{/if}}>Greater than or equal to</option>
          </select>
          <input name="amount" type="number" class="form-control w-auto" id="num-amount" value="{{ numerology.amount }}">
        </div>
      </div>

      <div class="form-group">
        <label for="num-emoji">Emoji</label>

        <div class="input-group">
          <input id="num-emoji" name="emoji" type="text" class="form-control" aria-describedby="num-emoji-help" value="{{ numerology.emoji }}">
          <div class="input-group-append">
            <button type="button" id="emoji-picker-btn" class="btn btn-outline-secondary">😊</button>
          </div>
        </div>
