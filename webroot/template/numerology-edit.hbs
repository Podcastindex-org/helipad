              <path d="M10.025 8a4.5 4.5 0 0 1-1.318 3.182L8 10.475A3.5 3.5 0 0 0 9.025 8c0-.966-.392-1.841-1.025-2.475l.707-.707A4.5 4.5 0 0 1 10.025 8M7 4a.5.5 0 0 0-.812-.39L3.825 5.5H1.5A.5.5 0 0 0 1 6v4a.5.5 0 0 0 .5.5h2.325l2.363 1.89A.5.5 0 0 0 7 12zM4.312 6.39 6 5.04v5.92L4.312 9.61A.5.5 0 0 0 4 9.5H2v-3h2a.5.5 0 0 0 .312-.11"/>
            </svg>
          </a>
          <input type="hidden" name="sound_file_existing" value="true">
        </div>
        {{/if}}
        <div id="num-sound-file-new" class="{{#if numerology.sound_file}}d-none{{/if}}">
          <input class="form-control" type="file" name="sound_file" accept=".mp3">
        </div>
        <small id="num-sound-help" class="form-text text-muted">Sound to play when boosts are received with this amount.</small>
      </div>
      <div class="form-group">
        <label for="num-position">Position</label>
        <input
          name="position"
          type="number"
          class="form-control w-auto"
          id="num-position"
          aria-describedby="num-position-help"
          {{#if numerology.position}}value="{{ numerology.position }}"{{/if}}
          {{#unless numerology.position}}value="1"{{/unless}}
        <small id="num-position-help" class="form-text text-muted">Numerologies with lower positions are applied first</small>
      </div>
      <div class="form-group">
        <label for="num-description">Description</label>
        <input name="description" type="text" class="form-control" id="num-description" aria-describedby="num-description-help" value="{{ numerology.description }}">
        <small id="num-description-help" class="form-text text-muted">Description to show in Helipad for this numerology.</small>
      </div>
    </div>
    <div class="modal-footer">
      <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      <button type="submit" class="btn btn-success">Save changes</button>
    </div>
  </div>
</div>
</form>

<script>
document.querySelectorAll('#emoji-picker span').forEach(btn => btn.addEventListener('click', () => {
    const input = document.querySelector('#num-emoji')
    input.value = input.value + btn.innerHTML
}))

document.querySelector('#emoji-picker-btn').addEventListener('click', () => {
    document.querySelector('#emoji-picker').classList.toggle('d-none')
})
</script>